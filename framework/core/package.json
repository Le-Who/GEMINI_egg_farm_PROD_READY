{
  "name": "@discord-activities/core",
  "version": "0.1.0",
  "description": "Core framework for building Discord Activities embedded games",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "@discord/embedded-app-sdk": "^1.0.0",
    "express": "^4.18.0",
    "phaser": "^3.60.0",
    "react": "^18.0.0 || ^19.0.0",
    "zod": "^3.20.0"
  },
  "peerDependenciesMeta": {
    "phaser": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "@discord/embedded-app-sdk": {
      "optional": true
    }
  },
  "devDependencies": {
    "@discord/embedded-app-sdk": "^1.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "express": "^4.18.2",
    "phaser": "^3.90.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.18",
    "zod": "^3.24.1"
  },
  "keywords": [
    "discord",
    "activities",
    "game",
    "framework",
    "embedded"
  ],
  "license": "MIT"
}

# Discord Application Credentials (from https://discord.com/developers/applications)
DISCORD_CLIENT_ID=your_client_id_here
DISCORD_CLIENT_SECRET=your_client_secret_here

# Must match "Redirects" in Discord Developer Portal > OAuth2
DISCORD_REDIRECT_URI=https://your-cloud-run-url.a.run.app

# Server port (Cloud Run sends PORT=8080 automatically)
PORT=8090

# Google Cloud Storage bucket for persistent data
# Leave empty for local-only mode (data lost on container restart)
GCS_BUCKET=
